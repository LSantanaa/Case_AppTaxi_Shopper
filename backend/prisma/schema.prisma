datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Drivers {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  car         String
  assessment  Float
  rateKM      Float
  minKM       Int
}
