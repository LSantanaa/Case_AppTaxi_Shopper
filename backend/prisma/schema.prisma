datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model TravelsHistory {
  id          Int      @id @default(autoincrement())
  custumer_id Int
  origin      String
  destination String
  distance    Int
  duration    String
  driver_id   Int
  driver_name String
  value       Float
  date        DateTime @default(now())
}
